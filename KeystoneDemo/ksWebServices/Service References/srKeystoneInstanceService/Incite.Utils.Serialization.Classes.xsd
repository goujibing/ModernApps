<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:ser="http://schemas.microsoft.com/2003/10/Serialization/" xmlns:tns="http://schemas.datacontract.org/2004/07/Incite.Utils.Serialization.Classes" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/Incite.Utils.Serialization.Classes" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://schemas.microsoft.com/2003/10/Serialization/" />
  <xs:import namespace="http://schemas.microsoft.com/2003/10/Serialization/Arrays" />
  <xs:import namespace="Incite" />
  <xs:complexType name="ArrayOfFileUploadResult">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="FileUploadResult" nillable="true" type="tns:FileUploadResult" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfFileUploadResult" nillable="true" type="tns:ArrayOfFileUploadResult" />
  <xs:complexType name="FileUploadResult">
    <xs:sequence>
      <xs:element minOccurs="0" name="FileGuid" type="ser:guid" />
