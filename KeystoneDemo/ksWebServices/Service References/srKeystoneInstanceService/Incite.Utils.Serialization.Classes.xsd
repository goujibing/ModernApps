    <xs:restriction base="xs:string">
      <xs:enumeration value="ReadyForDownload" />
      <xs:enumeration value="FileNotExist" />
      <xs:enumeration value="DownloadErrors" />
      <xs:enumeration value="FormInstanceInvalidated" />
      <xs:enumeration value="TokenInvalid" />
      <xs:enumeration value="ReportNotExist" />
      <xs:enumeration value="FileNoLongerExists" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="DownloadResultType" nillable="true" type="tns:DownloadResultType" />
  <xs:complexType name="FolderMetadata">
    <xs:sequence>
      <xs:element xmlns:q3="Incite" minOccurs="0" name="FileAttachment" nillable="true" type="q3:FA" />
