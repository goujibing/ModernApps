    </xs:sequence>
  </xs:complexType>
  <xs:element name="FileUploadResult" nillable="true" type="tns:FileUploadResult" />
  <xs:simpleType name="UploadResultType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Uploaded" />
      <xs:enumeration value="Virus" />
      <xs:enumeration value="NamingConvention" />
      <xs:enumeration value="OtherError" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="UploadResultType" nillable="true" type="tns:UploadResultType" />
  <xs:complexType name="DownloadResultList">
    <xs:sequence>
