      <xs:element minOccurs="0" name="ThumbnailForFilename" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="ThumbnailSizeType" type="tns:ThumbnailSizeType" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="FileThumbnailDownloadResult" nillable="true" type="tns:FileThumbnailDownloadResult" />
