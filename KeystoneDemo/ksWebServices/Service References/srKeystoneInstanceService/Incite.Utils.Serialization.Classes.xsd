    </xs:sequence>
  </xs:complexType>
  <xs:element name="DownloadResultList" nillable="true" type="tns:DownloadResultList" />
  <xs:complexType name="FileDownloadResult">
    <xs:sequence>
      <xs:element minOccurs="0" name="DownloadFrom" type="tns:DownloadFileFrom" />
      <xs:element minOccurs="0" name="FileGuid" type="ser:guid" />
      <xs:element minOccurs="0" name="FileLocation" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="FileName" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="ResultType" type="tns:DownloadResultType" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="FileDownloadResult" nillable="true" type="tns:FileDownloadResult" />
  <xs:simpleType name="DownloadFileFrom">
    <xs:restriction base="xs:string">
      <xs:enumeration value="S3" />
      <xs:enumeration value="SharedStorage" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="DownloadFileFrom" nillable="true" type="tns:DownloadFileFrom" />
