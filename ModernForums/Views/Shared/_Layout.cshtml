<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.9.0.min.js"></script>
    
    
    @Scripts.Render("~/Framework/AssetLoader.js")
    @Scripts.Render("~/Framework/BootUp.js")
    @Scripts.Render("~/Framework/DataLoader.js")
    @Scripts.Render("~/Framework/Debugger.js")
    @Scripts.Render("~/Framework/LanguageResources.js")
    @Scripts.Render("~/Framework/Scenes/MasterLayoutScreen.js")
    @Scripts.Render("~/Framework/SceneManager.js")
    @Scripts.Render("~/Framework/Theme.js")
    @Scripts.Render("~/Framework/UsageStats.js")
    @Scripts.Render("~/Framework/UIRenderer.js")


    @Scripts.Render("~/Framework/Controls/FrameworkControl.js")
    @Scripts.Render("~/Framework/Controls/LoadingControl.js")
    @Scripts.Render("~/Framework/Controls/AppBarControl.js")
    @Scripts.Render("~/Framework/Controls/ToolBarControl.js")
    @Scripts.Render("~/Framework/Controls/ToolBarItemControl.js")


</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    
</body>
</html>
